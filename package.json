{
  "name": "vite-vuessr",
  "description": "We're Vite in the VueSSR! -- The Beatles",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "VITE_STACK_MODE=spa vite --host",
    "build": "VITE_STACK_MODE=spa vite build",
    "serve": "VITE_STACK_MODE=spa vite preview",
    "serve:dist": "VITE_STACK_MODE=spa npm run build && sirv dist -s #needs `npm i sirv-cli -g`",
    "dev-ssr": "VITE_STACK_MODE=ssr vite-ssr dev",
    "build-ssr": "VITE_STACK_MODE=ssr vite-ssr build",
    "serve-ssr:dist": "VITE_STACK_MODE=ssr npm run build-ssr && node server.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@vee-validate/rules": "^4.5.7",
    "@vueuse/core": "^7.5.5",
    "@vueuse/head": "^0.7.5",
    "@vueuse/integrations": "^7.5.5",
    "axios": "^0.25.0",
    "pinia": "^2.0.23",
    "polka": "^0.5.2",
    "sirv": "^2.0.2",
    "sirv-cli": "^2.0.2",
    "universal-cookie": "^4.0.4",
    "vee-validate": "^4.5.8",
    "vite-ssr": "^0.15.0",
    "vue": "^3.2.29",
    "vue-draggable-next": "^2.1.1",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.4.3",
    "@heroicons/vue": "^1.0.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/tailwindcss": "^3.0.3",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/compiler-sfc": "^3.2.29",
    "autoprefixer": "^10.4.2",
    "tailwindcss": "^3.0.17",
    "vite": "^2.7.13"
  }
}
